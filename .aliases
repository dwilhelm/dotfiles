# term colors
TERMFG="#111111"
TERMBG="#f7f7dd"

# Set aliases.
alias ll='ls -lAF'
alias mplayer='mplayer -framedrop -stop-xscreensaver'
alias mutt='TERM=xterm-color mutt'
alias su='su -l'
alias sudo='sudo '
alias w3m='w3m -o wrap_search=true'
alias xterm='xterm -bg "$TERMBG" -fg "$TERMFG" +bdc +dc -sb -si -sl 256'
